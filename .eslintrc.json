{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [],
  "rules": {
    "no-unused-vars": "warn",
    "no-path-concat": "off",
    "camelcase": "warn",
    "no-array-constructor": "off"
  },
  "ignorePatterns": ["./.github/*"],
  "parserOptions": {
    // "parser": "@typescript-eslint/parser",
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "parser": "@typescript-eslint/parser"
}
